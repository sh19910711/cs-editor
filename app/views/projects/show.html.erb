<h1>Project: <%= @project.name %></h1>
<p><%= link_to 'Create New File', new_project_entity_path(@project) %></p>
<ul>
<% @project.entities.each do |e| %>
  <li><%= link_to e.path, project_entity_path(@project, e) %></li>
<% end %>
</ul>
<ul>
  <li><%= link_to 'Back to all projects', projects_path %></li>
</ul>
